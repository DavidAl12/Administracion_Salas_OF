@model Domain.Equipo

<h2>Eliminar Equipo</h2>
<p>¿Seguro que deseas eliminar el equipo <strong>@Model.Serial</strong>?</p>
<form method="post">
    <input type="hidden" asp-for="Id" />
    <button class="btn btn-danger">Eliminar</button>
    <a class="btn btn-secondary" href="/Equipo">Cancelar</a>
</form>
