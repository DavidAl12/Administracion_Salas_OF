@model Domain.Sala

<h2>Eliminar Sala</h2>
<h4>¿Está seguro que desea eliminar esta sala?</h4>
<dl class="row">
    <dt class="col-sm-2">Nombre</dt>
    <dd class="col-sm-10">@Model.Nombre</dd>
    <dt class="col-sm-2">Ubicación</dt>
    <dd class="col-sm-10">@Model.Ubicacion</dd>
    <dt class="col-sm-2">Estado</dt>
    <dd class="col-sm-10">@Model.Estado</dd>
    <dt class="col-sm-2">Capacidad</dt>
    <dd class="col-sm-10">@Model.Capacidad</dd>
</dl>
<form method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a href="/Sala" class="btn btn-secondary">Cancelar</a>
</form>
